{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES2017", // ან უფრო ახალი: ES2018, ES2020
    "lib": ["ES2017", "DOM"], // მოიცავს Object.entries და სხვა ES2017 ფუნქციებს
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "types": ["@types/react-native-dotenv"]
  }
}
