{
  "expo": {
    "name": "iq-night",
    "slug": "iq-night",
    "version": "1.0.0",
    "scheme": "iq-night",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "entryPoint": "./index.js",
    "sdkVersion": "52.0.0",
    "platforms": ["ios", "android"],
    "developmentClient": true,
    "backgroundColor": "#000",
    "notification": {
      "icon": "./assets/mafia5.jpg",
      "color": "#fff"
    },
    "splash": {
      "image": "./assets/bg.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iqnight.app",
      "newArchEnabled": false,
      "buildNumber": "1.0.9",
      "usesAppleSignIn": true,
      "keychainSharing": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mafia4.jpg",
        "newArchEnabled": false,
        "backgroundColor": "#000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "04bda34c-5ce2-4edd-804f-f4cdd1cb6c82"
      }
    },
    "owner": "iq-night",
    "plugins": ["expo-font"]
  }
}
